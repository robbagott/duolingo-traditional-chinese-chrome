{
  "name": "Duolingo Traditional Chinese",
  "description": "When active, converts simplified Chinese characters to traditional Chinese characters during Duolingo lessons.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "scripting", "storage"],
  "host_permissions": ["http://*.duolingo.com/*", "https://*.duolingo.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["makemeahanzi-dictionary.txt", "cedict.json"],
      "matches": [],
      "extension_ids": []
    }
  ]
}
